{
  "name": "sauce-cypress-runner",
  "version": "0.0.0",
  "description": "Sauce Labs test runner image for saucectl to run Cypress tests.",
  "author": "devx <dev@saucelabs.com>",
  "license": "MIT",
  "private": true,
  "bin": "./bin/cypress",
  "main": "./lib/cypress-runner.js",
  "homepage": "https://github.com/saucelabs/sauce-cypress-runner",
  "repository": {
    "type": "git",
    "url": "git://github.com/saucelabs/sauce-cypress-runner.git"
  },
  "scripts": {
    "test": "./bin/cypress",
    "test-integration": "bash tests/integration/integration-tests.sh",
    "build": "tsc",
    "lint": "prettier --check '**/*.{js,ts,mjs,cjs}' && eslint tests/ src/",
    "fmt": "prettier --write '**/*.{js,ts,mjs,cjs}'",
    "release": "release-it --github.release",
    "release:ci": "npm run release -- --ci --npm.skipChecks --no-git.requireCleanWorkingDir",
    "release:patch": "npm run release -- patch",
    "release:minor": "npm run release -- minor",
    "release:major": "npm run release -- major",
    "unit-test": "jest --env node --passWithNoTests"
  },
  "keywords": [],
  "dependencies": {
    "@babel/core": "7.24.1",
    "@babel/preset-env": "7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@badeball/cypress-cucumber-preprocessor": "20.0.2",
    "@cypress/grep": "4.0.1",
    "@cypress/webpack-preprocessor": "6.0.1",
    "@cypress/xpath": "2.0.3",
    "@saucelabs/cypress-junit-plugin": "0.2.0",
    "@saucelabs/cypress-plugin": "3.1.3",
    "@shelex/cypress-allure-plugin": "2.40.2",
    "@tsconfig/node20": "20.1.2",
    "babel-loader": "9.1.3",
    "cypress": "13.7.0",
    "cypress-mochawesome-reporter": "3.8.2",
    "cypress-multi-reporters": "1.6.4",
    "cypress-wait-until": "3.0.1",
    "dotenv": "16.4.5",
    "fluent-ffmpeg": "^2.1.2",
    "lodash": "4.17.21",
    "mkdirp": "^3.0.1",
    "playwright-webkit": "1.42.1",
    "sauce-testrunner-utils": "2.1.0",
    "typescript": "5.4.2",
    "webpack": "5.90.3",
    "xml-js": "^1.6.11",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.13",
    "@types/debug": "^4.1.12",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/jest": "^29.5.12",
    "@types/jquery": "^3.5.29",
    "@types/lodash": "^4.17.0",
    "@types/md5": "^2.3.5",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.30",
    "@types/xml": "^1.0.11",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "mocha": "^10.3.0",
    "prettier": "^3.2.5",
    "release-it": "^17.1.1"
  }
}
