    "webpack": "5.104.1",
    "xml-js": "^1.6.11",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@types/chai": "^5.2.3",
    "@types/debug": "^4.1.12",
    "@types/eslint-config-prettier": "6.11.3",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/jest": "^30.0.0",
    "@types/jquery": "^3.5.33",
    "@types/lodash": "^4.17.23",
    "@types/md5": "^2.3.6",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.8",
    "@types/xml": "^1.0.11",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
