    "eslint-plugin-jest": "^29.12.1",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "mocha": "^11.7.5",
    "prettier": "^3.7.4",
    "pretty-quick": "^4.2.2",
    "release-it": "^19.2.3",
    "typescript-eslint": "^8.53.0"
  }
